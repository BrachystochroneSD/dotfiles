    twitchvod=$(curl -s 'https://api.twitch.tv/kraken/channels/'$searchingshit'/videos?client_id='$clientID'&limit='$max_vid'&offset='$offset);     total_vod=$(echo $twitchvod | jq '._total');     [[ $total_vod == null || $total_vod == 0 ]] && twitchvod_search
